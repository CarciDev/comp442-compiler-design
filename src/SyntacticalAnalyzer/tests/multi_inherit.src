// Test: multiple inheritance and member access chains
class Node {
	public getValue() : integer;
	public setValue(integer v) : void;
	private integer value;
};

class LinkedList {
	public insert(integer v) : void;
	public search(integer v) : integer;
	public getSize() : integer;
	private Node head;
	private integer size;
};

class SortedList inherits LinkedList, Node {
	public insertSorted(integer v) : void;
};

Node::getValue() : integer
  do
    return (value);
  end;

Node::setValue(integer v) : void
  do
    value = v;
  end;

LinkedList::insert(integer v) : void
  do
    head.setValue(v);
    size = size + 1;
  end;

